/dts-v1/;

/include/ "omap5.dtsi"

/ {
	model = "TI OMAP5 PandaBoard";
	compatible = "ti,omap5-panda", "ti,omap5432", "ti,omap5";

	chosen {
		bootargs = "console=ttyO2,115200n8 no_console_suspend debug";
	};

	memory {
		device_type = "memory";
		reg = <0x80000000 0x40000000>; /* 1 GB */
	};
};
